package nc.ui.dip.cmdctl;

import java.io.Serializable;
import java.util.ArrayList;
import java.util.List;

public class CtlUtilVo implements Serializable{
	/**
	 * 
	 */
	private static final long serialVersionUID = -5280073074230721875L;
	public int colCount;
	private List columnTypes = new ArrayList();
	private List columnNames = new ArrayList();
	public List result = new ArrayList();
	public int rowCount;
	
	
	public int getColCount() {
		return colCount;
	}
	public void setColCount(int colCount) {
		this.colCount = colCount;
	}
	public List getColumnNames() {
		return columnNames;
	}
	public void setColumnNames(List columnNames) {
		this.columnNames = columnNames;
	}
	public List getColumnTypes() {
		return columnTypes;
	}
	public void setColumnTypes(List columnTypes) {
		this.columnTypes = columnTypes;
	}
	public List getResult() {
		return result;
	}
	public void setResult(List result) {
		this.result = result;
	}
	public int getRowCount() {
		return rowCount;
	}
	public void setRowCount(int rowCount) {
		this.rowCount = rowCount;
	}
}
